{
  "manifest_version": 2,

  "name": "GitHub markdown ToC",
  "description": "Generate toc of github markdown automatically",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["js/jquery.min.js", "js/main.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}